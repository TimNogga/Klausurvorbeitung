   "source": [
    "from numpy import *\n",
    "from numpy.random import rand\n",
    "from numpy.linalg import norm, inv\n",
    "M = rand(4,4)\n",
    "A = diag([3.0,2.0,-3.0,0.0])\n",
    "u = rand(4,1)\n",
    "u = u/norm(u)\n",
    "m = 1.8\n",
    "B = inv(A - m * eye(4,4))\n",
    "v = u\n",
    "for i in range(100):\n",
    "    w = dot(B,v)\n",
    "    v = w/norm(w)\n",
    "l = dot(v.T, dot(A,v))\n",
    "print(l)\n"
   ]
